<h1>Sessions#new</h1>
<p>Find me in app/views/sessions/new.html.erb</p>

<%= form_for @user, url: "/sessions" do |f| %>
	<div>
		<%= f.text_field :email, placeholder: "Email" %>
	</div>
	<div>
		<%= f.text_field :password, placeholder: "Password" %>
	</div>
	<div>
		<%= f.submit "LOGIN" %>
	</div>
<% end %>